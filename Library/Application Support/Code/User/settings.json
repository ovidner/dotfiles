{
  "editor.fontFamily": "'SFMono-Regular'",
  "editor.tabSize": 2,
  "editor.rulers": [
    80,
    120
  ],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "python.venvPath": "~/.virtualenvs",
  "workbench.iconTheme": "vs-seti",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true
  }
}
